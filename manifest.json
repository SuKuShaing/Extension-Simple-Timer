{
    "manifest_version": 3,
    "name": "Simple Timer",
    "version": "1.3",
    "description": "a Simple Timer",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "permissions": [
        "notifications",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    }
}