{
    "manifest_version": 3,
    "name": "Mi Extensión de Ejemplo",
    "version": "1.0",
    "description": "Una extensión simple que muestra un popup.",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "permissions": [
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    }
}